		<variable name="toolchain" value="caanoo"/>
	</target>
	<target name="sound-wiz" extends="sound-mac">
		<option warning="true" is_attached_by_default="false" syntax="transitional">
			<assertions/>
		</option>
		<variable name="toolchain" value="wiz"/>
	</target>
</system>
